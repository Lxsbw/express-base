{
  "name": "express-base",
  "version": "0.0.1",
  "description": "Node.js & Express & MVC & mongoose & swagger",
  "private": true,
  "type": "module",
  "scripts": {
    "setES" : "NODE_OPTIONS=--experimental-specifier-resolution=node",
    "start": "npm run setES node ./bin/www.js",
    "dev": "npm run setES ./node_modules/.bin/nodemon bin/www.js",
    "prdold": "pm2 start bin/www.js --name=express-base",
    "prd": "pm2 start 'npm run setES node bin/www.js' --name=express-base",
    "stop": "pm2 stop express-base && pm2 delete express-base",
    "del": "rm -rf node_modules && rm -rf yarn.lock"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-swagger-generator": "~1.1.17",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "~5.11.8",
    "morgan": "~1.9.1",
    "nodemon": "~2.0.6"
  }
}
