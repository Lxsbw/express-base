{
  "name": "express-base",
  "version": "0.0.1",
  "description": "Node.js & Express & MVC & mongoose & swagger",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www --name=express-base",
    "stop": "pm2 stop express-base && pm2 delete express-base",
    "del": "rimraf node_modules dist yarn.lock"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "express-swagger-generator": "~1.1.17",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jslinq": "^1.0.22",
    "linq-to-javascript": "^2.0.0",
    "mongoose": "~7.0.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "~2.0.6",
    "rimraf": "~4.2.0"
  }
}
