{
  "name": "express-base",
  "version": "0.0.1",
  "description": "Node.js & Express & MVC & mongoose & swagger",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www --name=express-base",
    "stop": "pm2 stop express-base && pm2 delete express-base",
    "del": "rm -rf node_modules && rm -rf yarn.lock"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-swagger-generator": "^1.1.17",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jslinq": "^1.0.22",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.8",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6"
  }
}
